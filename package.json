{
  "name": "d-reader-backend",
  "version": "1.0.0",
  "description": "NestJS backend for dReader dapp on Solana",
  "author": "",
  "private": false,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.x",
    "yarn": "1.x",
    "npm": "9.x"
  },
  "scripts": {
    "prebuild": "rimraf dist && npx prisma generate",
    "build": "nest build",
    "postbuild": "cp -r public dist/public",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "heroku:db": "npx prisma db push",
    "migrate:dev": "npx prisma migrate dev",
    "seed": "npx prisma db seed",
    "generate-env": "npx ts-node src/cli/main.ts generate-environment",
    "create-ah": "npx ts-node src/cli/main.ts create-auction-house",
    "sync-webhook": "npx ts-node src/cli/main.ts sync-webhook",
    "sync-wallet": "npx ts-node src/cli/main.ts sync-wallet",
    "airdrop-sol": "npx ts-node src/cli/main.ts airdrop-sol",
    "login-user": "npx ts-node src/cli/main.ts login-user",
    "mint-one": "npx ts-node src/cli/main.ts mint-one",
    "mint-remaining": "npx ts-node src/cli/main.ts mint-remaining",
    "add-allow-list": "npx ts-node src/cli/main.ts add-allow-list",
    "thaw-collection": "npx ts-node src/cli/main.ts thaw-collection",
    "bundlr-withdraw": "npx ts-node src/cli/main.ts bundlr-withdraw",
    "bundlr-fund": "npx ts-node src/cli/main.ts bundlr-fund",
    "add-group": "npx ts-node src/cli/main.ts add-group",
    "add-collection-darkblock": "npx ts-node src/cli/main.ts add-collection-darkblock",
    "update-date": "npx ts-node src/cli/main.ts update-date",
    "delegate-creator": "npx ts-node src/cli/main.ts delegate-creator",
    "sync-collection": "npx ts-node src/cli/main.ts sync-collection",
    "delete-candy-machine": "npx ts-node src/cli/main.ts delete-candy-machine"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.370.0",
    "@aws-sdk/client-s3": "^3.370.0",
    "@aws-sdk/lib-storage": "^3.370.0",
    "@aws-sdk/s3-request-presigner": "^3.370.0",
    "@bonfida/spl-name-service": "^1.1.1",
    "@discord-nestjs/common": "^5.2.10",
    "@discord-nestjs/core": "^5.3.12",
    "@metaplex-foundation/js": "^0.19.4",
    "@metaplex-foundation/mpl-auction-house": "^2.5.0",
    "@metaplex-foundation/mpl-candy-guard": "^0.5.0",
    "@metaplex-foundation/mpl-candy-machine-core": "^0.2.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/platform-socket.io": "^10.1.3",
    "@nestjs/schedule": "^3.0.2",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/websockets": "^10.1.3",
    "@prisma/client": "^5.1.1",
    "@project-serum/anchor": "^0.26.0",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.78.0",
    "@types/ejs": "^3.0.3",
    "@types/inquirer": "^9.0.3",
    "@types/pug": "2.0.6",
    "@types/uuid": "^9.0.2",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bs58": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "discord.js": "^14.14.1",
    "dreader-comic-verse": "^3.0.0",
    "ejs": "^3.1.2",
    "firebase-admin": "^12.0.0",
    "geoip-lite": "^1.4.7",
    "handlebars": "^4.7.6",
    "helius-sdk": "^1.0.16",
    "jdenticon": "^3.2.0",
    "lodash": "^4.17.21",
    "nest-commander": "^3.11.0",
    "nestjs-prisma": "^0.22.0",
    "nestjs-real-ip": "^3.0.1",
    "nodemailer": "^6.9.3",
    "p-ratelimit": "^1.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "prisma-dbml-generator": "^0.10.0",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "timekeeper": "^2.2.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "ua-parser-js": "^1.0.34",
    "uuid": "^9.0.0",
    "webpack": "^5.88.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^10.1.11",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.1.3",
    "@noble/ed25519": "^2.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^2.0.1",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.195",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.10",
    "@types/nodemailer": "^6.4.8",
    "@types/supertest": "^2.0.11",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "chalk": "^4.1.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.1",
    "prisma": "^5.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
